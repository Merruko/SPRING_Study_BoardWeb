<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	
	<!-- Alias 설정 -->
	<typeAliases>
		<typeAlias alias="board" type="com.biz.vo.board.BoardVO" />
		<typeAlias alias="boardResult" type="com.biz.vo.board.BoardVO" />
		<typeAlias alias="user" type="com.biz.vo.user.UserVO" />
	</typeAliases>
	
	<!-- SQL Mapper 설정 -->
	<mappers>
		<mapper resource="mapper/boardMapper.xml" />
		<mapper resource="mapper/userMapper.xml" />
	</mappers>
	
</configuration>